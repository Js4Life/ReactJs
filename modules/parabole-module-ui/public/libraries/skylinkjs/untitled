<!DOCTYPE html>
<html>
  <head>
  </head>
  <body>
    <video id="me" autoplay="autoplay" muted="muted"></video>
    <script src="../../publish/skylink.complete.js"  type="text/javascript"></script>
    <script src="../config.js"   type="text/javascript"></script>
    <script>
       var skylink = new Skylink();

      skylink.getUserMedia({
        audio: true,
        video: true
      }, function (stream) {
        attachMediaStream(document.getElementById('me'), stream);
      }, function (error) {
        throw error;
      });
    </script>
  </body>
</html>
