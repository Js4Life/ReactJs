# An example controller showing a sample home page

GET /getRawBindingDataValues/:fileName                     com.parabole.feed.application.controllers.FeedController.getRawBindingDataValues(fileName: String)
GET /getTheassignments                     com.parabole.feed.application.controllers.FeedController.getTheassignments()
GET /startContentParser/:fileName                     com.parabole.feed.application.controllers.FeedController.startContentParser(fileName: String)


# Map static resources from the /public folder to the /assets URL path
GET      /feedassets/*file        controllers.Assets.at(path="/public/lib/parabole-module-feed", file)
