PREFIX mpb-root-main: <http://www.mindparabole.com/ontology/finance/MPBRoot_Main#> 
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX cit:<http://www.mindparabole.com/ontology/finance/Bank_Main#>
PREFIX mpb-fin-main-ccar-14q:<http://www.mindparabole.com/ontology/finance/FRY-14Q_Main#>
PREFIX mpb-root-main-crm:<http://www.mindparabole.com/ontology/finance/CreditRiskManagement_Main#>
PREFIX mpb-fin-main-ccar:<http://www.mindparabole.com/ontology/finance/CCAR_Main#>
PREFIX xsd:<http://www.w3.org/2001/XMLSchema#>
SELECT ?reportField ?labelreportField ?reportFieldStatus ?interim ?priority ?remediation ?sub_schedule ?strategic
WHERE {
    	    	mpb-fin-main-ccar:Sch_H_Wholesale_Risk mpb-root-main:hasReportSub_Schedule ?sub_schedule .
?sub_schedule  mpb-root-main:hasReportField ?reportField .
OPTIONAL {?reportField mpb-root-main:hasReportFieldStatus ?reportFieldStatus .}
OPTIONAL {?reportField mpb-root-main:reportFieldRemediation ?remediation .}
OPTIONAL {?reportField mpb-root-main:reportFieldInterimStatus ?interim .}
OPTIONAL {?reportField mpb-root-main:reportFieldPriority ?priority .}
OPTIONAL {?reportField mpb-root-main:reportFieldStrategicStatus ?strategic .} 
	OPTIONAL {?reportField rdfs:label ?labelreportField .}
	?sub_schedule rdfs:label ?labelReportSub_Schedule .

	
}

ORDER BY ?reportField